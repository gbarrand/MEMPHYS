<widget class="CascadeButton" name="OnX_Plotter_casc">
  <label>Plotter</label>
  <widget class="Menu">
    <widget class="MenuItem">
      <label>New plotting page</label>
      <activate exec="DLD">OnX ui_createComponent</activate>
      <activate exec="DLD">OnX page_createRegions 1 1 0 SoPlotterRegion</activate>
    </widget>
    <widget class="MenuItem">
      <label>Close MEMPHYS.root file</label>
      <activate exec="DLD">MEMPHYS MEMPHYS_analysis_closeTree</activate>
    </widget>
    <widget class="MenuItem">
      <label>Open ROOT file...</label>
      <activate exec="DLD">OnX ui_showFileChooser Lab Lab_File_ROOT_open *.root $LABROOT/examples/data</activate>
    </widget>
    <reference file="$ONXROOT/scripts/OnX/Plotter.onx"/>
  </widget>
</widget>
