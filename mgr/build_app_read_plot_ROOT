#!/bin/sh -f

. ./find_bush

. ${bush_home}/args

. ${bush_home}/ourex_header

#//////////////////////////////////////////////////////
#//////////////////////////////////////////////////////
#//////////////////////////////////////////////////////
cppflags="${cppflags} -DINLIB_USE_CSZ"

use_inlib=yes
use_exlib=yes
use_csz=yes
use_zlib=yes
use_cern_root=yes

. ${bush_home}/use/inlib
. ${bush_home}/use/exlib
. ${bush_home}/use/csz
. ${bush_home}/use/zlib
. ${bush_home}/use/cern_root

#//////////////////////////////////////////////////////
#//////////////////////////////////////////////////////
#//////////////////////////////////////////////////////

objs=
app_exe=MEMPHYS_read_plot_ROOT
app_src=../applications/read_plot_ROOT.cpp

. ${bush_home}/application

. ${bush_home}/clean

