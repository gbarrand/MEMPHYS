#ifndef inlib_wroot_imutex
#define inlib_wroot_imutex

namespace inlib {
namespace wroot {

class imutex {
public:
  virtual ~imutex(){}
public:
  virtual bool lock() = 0;
  virtual bool unlock() = 0;
  //virtual bool trylock() = 0;
};

}}

#endif
